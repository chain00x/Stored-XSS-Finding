{
  "name": "Injecta",
  "version": "0.0.1",
  "manifest_version": 2,
  "description": "Injecting stuff",
  "background": {
    "scripts": [
      "background.js"
    ],
    "persistent": true
  },
  "browser_action": {
  "default_icon": "1.png",
  "default_title": "icon"
    },
  "permissions": [
    "https://*/*",
    "http://*/*",
    "tabs"
  ]
}
